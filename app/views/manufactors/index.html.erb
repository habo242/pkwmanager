<h1>Listing manufactors</h1>

<table>
  <thead>
    <tr>
      <th>Manufactor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manufactors.each do |manufactor| %>
      <tr>
        <td><%= manufactor.manufactor %></td>
        <td><%= link_to 'Show', manufactor %></td>
        <td><%= link_to 'Edit', edit_manufactor_path(manufactor) %></td>
        <td><%= link_to 'Destroy', manufactor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manufactor', new_manufactor_path %>
